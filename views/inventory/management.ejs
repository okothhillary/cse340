<div class="management">
    <% if (title) { %>
      <h1><%= title %></h1>
    <% } else { res.redirect('/') } %>
  
    <%- messages() %>
  
    <% if (errors) { %>
      <ul class="notice">
        <% errors.array().forEach(error => { %>
          <li><%= error.msg %></li>
        <% }) %>
      </ul>
    <% } %>
  
    <ul>
      <li>
        <a href="/inv/add-classification">Add New Classification</a>
      </li>
      <li>
        <a href="/inv/add-inventory">Add New Inventory</a>
      </li>
    </ul>
  </div>
  